

<%
@days = {}
@votes.each do |vote|
if vote.vote
  @rating = @rating + 1 
else
  @rating = @rating - 1 
end

logger.info vote.created_at.strftime('%Y-%m-%d')

if @days[vote.created_at.strftime('%Y-%m-%d')] == nil
  @days[vote.created_at.strftime('%Y-%m-%d')] = 0
else
  @days[vote.created_at.strftime('%Y-%m-%d')] += @rating
end
end

%>

<%= @days %>