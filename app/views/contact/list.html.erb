<h1>Listing contacts</h1>

<table>
  <tr>
    <th>Email</th>
    <th>College / universite</th>
    <th>Comment</th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.email %></td>
    <td><%= contact.college %></td>
    <td><%= contact.comment_text %></td>
  </tr>
<% end %>
</table>

<br />

